<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jalen.animator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/animator_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.jalen.animator.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jalen.animator.activity.transition.TransitionBaseUseActivity"
            android:label="Transition的基本使用" />
        <activity android:name="com.jalen.animator.activity.Demo1Activity" />
        <activity android:name="com.jalen.animator.activity.interpolator.InterpolatorActivity" />
        <activity
            android:name=".activity.transition.TransitionAddTargetActivity"
            android:label="Transition指定动画执行者" />
        <activity
            android:name=".activity.transition.TransitionDelayActivity"
            android:label="Transition延时动画" />

        <activity
            android:name=".activity.transition.customtransition.TransitionCustomColorActivity"
            android:label="颜色改变的动画" />

        <activity
            android:name=".activity.transition.pagecontent.PageContentFirstActivity"
            android:label="页面内容过渡动画页面一" />

        <activity
            android:name=".activity.transition.pagecontent.PageContentSecondActivity"
            android:label="页面内容过渡动画页面二" />

        <activity
            android:name=".activity.transition.shareelement.ShareElementFirstActivity"
            android:label="共享元素页面一" />

        <activity
            android:name=".activity.transition.shareelement.ShareElementSecondActivity"
            android:label="共享元素页面二" />

    </application>

</manifest>